{% extends 'base.html.twig' %}

{% block title %}Photo{% endblock %}

{% block body %}
    <h1>Photo</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ photo.id }}</td>
            </tr>
            <tr>
                <th>StorageName</th>
                <td>{{ photo.storageName }}</td>
            </tr>
            <tr>
                <th>CreateAt</th>
                <td>{{ photo.createAt ? photo.createAt|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>FileType</th>
                <td>{{ photo.fileType }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_photo_index') }}">back to list</a>

    <a href="{{ path('app_photo_edit', {'id': photo.id}) }}">edit</a>

    {{ include('photo/_delete_form.html.twig') }}
{% endblock %}
